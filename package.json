  "bugs": {
    "url": "https://github.com/utropicmedia/storj-nodejs/issues"
  },
  "homepage": "https://github.com/utropicmedia/storj-nodejs#readme",
  "dependencies": {
    "bindings": "^1.5.0",
    "node-gyp": "^6.1.0"
